[sub_resource type="GDScript" id=5]
script/source = "extends ColorRect


func _ready():
	if !OS.has_feature(\"standalone\"):
		queue_free()
	$LighterSound.play()
	yield(get_tree().create_timer(0.7), \"timeout\")
	$AnimationPlayer.play(\"To Left\")
	yield(get_tree().create_timer(0.1), \"timeout\")	
	$FireSound.play()
#	$Tween.interpolate_property($Sprite, \"modulate\", Color(\"00ffffff\"), Color(\"ffffff\"), 0.8)
#	$Tween.start()
	


func _on_AnimationPlayer_animation_finished(anim_name):
	if anim_name == \"To Left\":
		yield(get_tree().create_timer(1), \"timeout\")
#		owner.get_node(\"Actions\").show()
		$Tween.interpolate_property(self, \"modulate\", modulate, Color(\"00ffffff\"), 0.6)
		$Tween.start()



func _on_FireSound_finished():
	queue_free()
"

