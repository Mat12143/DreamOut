script/source = "extends Control
export var disableForDebug = true

func _ready():
	print(\"TODO: Fixare HUD nello spawn, anzi toglierla proprio se non per la chat.\")
	
	randomize()
	var titles = [
		\"Capybaras are always valid\",
		\"made by pyros.dev\",
		\"Ok lets go\",
		\"Avoid PHP at all costs\",
		\"Look mother, I am a hat\",
		\"No autoclickers allowed\",
		\"Officina Tecnologica\",
		\"John China\",
		\"Imagine using joomla...\",
		\"Hilarious\",
		\"Godot Engine\",
		\"Hehehe HA\",
		\"What a giullare...\",
		\"Ole ole\",
		\"I swear\",
		\"Check out pyros.dev\"
	]
	OS.set_window_title(\"Dreamout: %s!\" % RNGTools.pick(titles))
	
	if !OS.has_feature(\"standalone\"):
		if disableForDebug:
			get_tree().change_scene(\"res://MatWorld.tscn\")
		
		if \"listen\" in OS.get_cmdline_args():
			$Actions/NameInput.text = \"Tro\"
			$Actions/NameInput.emit_signal(\"text_changed\", \"Tro\")
		elif \"join\" in OS.get_cmdline_args():
			$Actions/NameInput.text = \"SLAVE\"
			$Actions/NameInput.emit_signal(\"text_changed\", \"SLAVE\")			
		
		
"

